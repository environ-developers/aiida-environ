{
    "author": "Matthew Truscott",
    "author_email": "matthew.a.truscott@gmail.com",
    "classifiers": [
        "Programming Language :: Python :: 3.7",
        "Programming Language :: Python :: 3.8",
        "Development Status :: 0 - Alpha"
    ],
    "description": "Environ plugin",
    "entry_points": {
        "aiida.calculations": [
            "environ.pw = aiida_environ.calculations.pw:EnvPwCalculation"
        ],
        "aiida.data": [
            "environ.charges = aiida_environ.data.charge:EnvironChargeData"
        ],
        "aiida.parsers": [
            "environ.pw = aiida_environ.parsers.pw:EnvPwParser"
        ],
        "aiida.workflows": [
            "environ.pw.base = aiida_environ.workflows.pw.base:EnvPwBaseWorkChain",
            "environ.pw.relax = aiida_environ.workflows.pw.relax:EnvPwRelaxWorkChain",
            "environ.pw.solvation = aiida_environ.workflows.pw.solvation:PwSolvationWorkChain",
            "environ.pw.parameterization = aiida_environ.workflows.pw.parameterization:ParameterizationWorkChain",
            "environ.pw.graphml = aiida_environ.workflows.pw.adsorbate:AdsorbateGraphConfiguration",
            "environ.pw.grandcanonical = aiida_environ.workflows.pw.grandcanonical:AdsorbateGrandCanonical",
            "environ.pw.test_force = aiida_environ.workflows.pw.test_force:EnvPwForceTest"
        ]
    },
    "extras_require": {
        "pre-commit": [
            "pre-commit~=2.2",
            "pylint~=2.5.0"
        ],
        "tests": [
            "pgtest~=1.3",
            "pytest~=5.4",
            "pytest-regressions~=1.0"
        ],
        "tcod": [
            "aiida-tcod"
        ],
        "docs": [
            "Sphinx",
            "docutils",
            "sphinx_rtd_theme"
        ]
    },
    "install_requires": [
        "aiida_core>=1.6",
        "aiida_quantumespresso>=3.4",
        "packaging",
        "xmlschema~=1.2"
    ],
    "license": "",
    "name": "aiida_environ",
    "python_requires": ">=3.5",
    "url": "",
    "version": "0.2.0"
}
