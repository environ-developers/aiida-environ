{
    "author": "Matthew Truscott",
    "author_email": "matthew.a.truscott@gmail.com",
    "classifiers": [
        "Programming Language :: Python :: 3.7",
        "Programming Language :: Python :: 3.8",
        "Development Status :: 0 - Alpha"
    ],
    "description": "Environ plugin",
    "entry_points": {
        "aiida.calculations": [
            "environ.pw = aiida_environ.calculations.pw:EnvPwCalculation"
        ],
        "aiida.parsers": [
            "environ.pw = aiida_environ.parsers.pw:EnvPwParser"
        ],
        "aiida.workflows": [
            "environ.pw.base = aiida_environ.workflows.pw.base:EnvPwBaseWorkChain",
            "environ.pw.relax = aiida_environ.workflows.pw.relax:EnvPwRelaxWorkChain"
        ]
    },
    "extras_require": {
        "pre-commit": [
            "pre-commit~=2.2",
            "pylint~=2.5.0"
        ],
        "tests": [
            "pgtest~=1.3",
            "pytest~=5.4",
            "pytest-regressions~=1.0"
        ],
        "tcod": [
            "aiida-tcod"
        ],
        "docs": [
            "Sphinx",
            "docutils",
            "sphinx_rtd_theme"
        ]
    },
    "install_requires": [
        "aiida_core[atomic_tools]~=1.2",
        "aiida_quantumespresso~=3.0",
        "packaging",
        "qe-tools~=1.1",
        "xmlschema~=1.2"
    ],
    "license": "",
    "name": "aiida_environ",
    "python_requires": ">=3.5",
    "url": "",
    "version": "0.1.0"
}
